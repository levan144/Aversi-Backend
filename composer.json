{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alexazartsev/heroicon": "^2.0",
        "bolechen/nova-activitylog": "^0.4.0",
        "dniccum/phone-number": "^2.0",
        "ebess/advanced-nova-media-library": "^4.0",
        "eminiarts/nova-tabs": "^2.2",
        "formfeed-uk/nova-breadcrumbs": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "itsmejoshua/novaspatiepermissions": "^1.0",
        "kongulov/nova-tab-translatable": "^2.0",
        "laravel/framework": "^8.75",
        "laravel/helpers": "^1.5",
        "laravel/nova": "4.12.1",
        "laravel/passport": "^10.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "michielfb/laravel-nova-time-field": "^1.2",
        "murdercode/nova4-tinymce-editor": "^1.2",
        "outl1ne/nova-menu-builder": "^7.0",
        "outl1ne/nova-multiselect-field": "^4.0",
        "outl1ne/nova-settings": "^5.0",
        "outl1ne/nova-translatable": "^2.2",
        "protonemedia/laravel-cross-eloquent-search": "^3.0",
        "sadekd/nova-opening-hours-field": "^3.0",
        "shuvroroy/nova-tabs": "^1.1",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-health": "^1.9",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-translatable": "^5.0",
        "spatie/laravel-translation-loader": "^2.7",
        "spatie/opening-hours": "^2.13",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "stepanenko3/nova-json": "^4.0",
        "stepanenko3/nova-settings": "^1.0",
        "webklex/php-imap": "2.2.5",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }        
    }
}
